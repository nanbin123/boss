/* pages/ personalData/ personalData.wxss */
page{
  width: 100%;
}
.personalData{
  width: 100%;
  padding: 0 40rpx;
  box-sizing: border-box;
}
.nameData {
  width: 100%;
  height: 150rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 4rpx solid #f2f2f2;
}

.personalcontent {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.name {
  display: block;
  font-size: 60rpx;
  font-weight: bold;
}

.jobStatus {
  color: #5c5c5c;
  font-size: 34rpx;
  margin-left: 60rpx;
}
.state{
  width: 100%;
  border-bottom: 4rpx solid #f2f2f2;
  padding-bottom: 50rpx;

}


.stateYears{
  padding-bottom: 30rpx;
  padding-top: 30rpx;
}
.iconfont{
  font-size: 32rpx;
  display: inline-block;
  margin-right: 30rpx;
  color: #5c5c5c
}
.item{
  display: inline-block;
  margin-left: 10rpx;
}
.stateNav{
  color: #5c5c5c;
  font-size: 34rpx;
}
.Resume {
  width: 100%;
  padding-bottom: 60rpx;
  border-bottom: 4rpx solid #f2f2f2;
}

.circle{
  width: 5px;
  height: 5px;
  background-color: #d3361a;
  border-radius: 50%;
  position: relative;
  left: -30rpx;
  top: 60rpx;
}


.title {
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
}
.titleText{
  font-size: 37rpx;
}

.jobExpectations {
  width: 100%;
  padding-bottom: 60rpx;
  border-bottom: 4rpx solid #f2f2f2;
}


.expectationsbox {
  width: 100%;
}

.expectationsItem {
  font-weight: bold;
  font-size: 34rpx;
  margin-right: 30rpx;
}
.money {
  display: block;
  float: right;
  color: #d3361a;
  font-size: 30rpx;
  font-weight: bold;
  padding: 5rpx 0;
}



.workExperience{
  width: 100%;
  padding-bottom: 60rpx;
  border-bottom: 4rpx solid #f2f2f2;
}
.experienceContentTitle{
  padding-bottom: 30rpx;
}
/* .titleIco1 {
  display: block;
  float: right;
  color: #646363;
  font-size: 30rpx;
} */
.serviceTime{ 
  font-size: 36rpx;
  margin-bottom: 20rpx;
  color: #6d6d6d;
}

.positionName{
  display: inline-block;
  width: 40%;
  font-size: 36rpx;
  margin-bottom: 20rpx;
  color: #6d6d6d;
}
.Strength {
  margin-top: 10rpx;
  font-size: 32rpx;
  line-height: 50rpx;
  color: #6d6d6d;
  width: 100%;
}
.dian{
  display: inline-block;
  width: 8rpx;
  height: 8rpx;
  background-color: #6d6d6d;
  border-radius: 50%;
  margin: 6rpx 16rpx;
}
.mt20rpx{
  margin-top: 20rpx;
}
.experience{
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}
.edu {
  margin-top: 10rpx;
  font-size: 32rpx;
  color: #6d6d6d;
  width: 100%;
}
.educationExperience{
  width: 100%;
  padding-bottom: 200rpx;
  border-bottom: 4rpx solid #f2f2f2;
}
.button{
  background: #fff;
  width: 100%;
  height: 160rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  padding: 0 20rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  border-top: 8rpx solid #f5f5f5;
}

button{
    width: 100%;
    color: #fff;
    background: #d3361a;
}
